# Configurações da API Backend
REACT_APP_API_URL=http://localhost:8000

# Configurações do Banco Principal (PostgreSQL)
REACT_APP_MAIN_DB_HOST=localhost
REACT_APP_MAIN_DB_PORT=5432
REACT_APP_MAIN_DB_NAME=dualm_principal
REACT_APP_MAIN_DB_USER=seu_usuario
REACT_APP_MAIN_DB_PASSWORD=sua_senha

# Ambiente
REACT_APP_ENV=development

# Timeout das requisições (em milissegundos)
REACT_APP_API_TIMEOUT=30000

# Configurações de debug
REACT_APP_DEBUG=true
